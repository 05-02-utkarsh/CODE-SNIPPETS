//if vector=[1,2,3]=>reverse integer =321

#include<bits/stdc++.h>
using namespace std;
int main(){
    vector<int>v;
    v.push_back(1);
    v.push_back(2);
    v.push_back(3);
    
    
    for(auto it:v){
        cout<<it<<" ";
    }
    
    cout<<endl;
    
    int sum=0;
    int x=1;
        for(auto n:v){
            
            sum+=n*x;
            x*=10;
        }
        
        cout<<sum;
    
}
